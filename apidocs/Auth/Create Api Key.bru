meta {
  name: Create Api Key
  type: http
  seq: 2
}

post {
  url: {{APP_URL}}/{{APP_VERSION}}/auth/keys
  body: none
  auth: inherit
}

headers {
  x-admin-key: {{x-admin-key}}
}

script:post-response {
  bru.setEnvVar('API_KEY',res.body.id);
  bru.setEnvVar('API_SECRET',res.body.secret);
}

settings {
  encodeUrl: true
}
